{
  "name": "puglite",
  "version": "1.0.0",
  "description": "A clean, whitespace-sensitive template language for writing HTML",
  "keywords": [
    "html",
    "jade",
    "pug",
    "template"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/raykin/puglite"
  },
  "main": "lib",
  "builders": "builders.json",
  "exports": {
    ".": "./lib/index.js",
    "./angular-webpack": "./angular-webpack/index.js",
    "./builders/browser": "./builders/browser/index.js",
    "./builders/dev-server": "./builders/dev-server/index.js",
    "./builders.json": "./builders.json",
    "./package.json": "./package.json"
  },
  "files": [
    "lib/",
    "angular-webpack/",
    "builders.json",
    "builders/"
  ],
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "watch": "jest --watch"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.js"
    ]
  },
  "dependencies": {
    "character-parser": "^4.0.0",
    "constantinople": "^4.0.1",
    "doctypes": "^1.1.0",
    "is-expression": "^4.0.0",
    "js-stringify": "^1.0.2",
    "token-stream": "^1.0.0",
    "void-elements": "^3.1.0",
    "with": "^7.0.2"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^20.0.0",
    "jest": "^29.7.0",
    "mkdirp": "^0.5.1",
    "rimraf": "^3.0.2",
    "uglify-js": "^3.19.3"
  },
  "peerDependencies": {
    "@angular-builders/custom-webpack": ">=18.0.0",
    "@angular-devkit/architect": ">=0.1800.0",
    "@angular-devkit/build-angular": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "@angular-builders/custom-webpack": {
      "optional": true
    }
  },
  "browser": {
    "fs": false
  }
}
